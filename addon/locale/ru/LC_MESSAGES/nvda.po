#: clip_manager/__init__.py:139
msgid "Clipboard History"
msgstr "История буфера обмена"

#. Translators: Label for the paste button in the clipboard history window.
#: clip_manager/__init__.py:152
msgid "Paste"
msgstr "Вставить"

#. Translators: Label for the edit item in the context menu.
#: clip_manager/__init__.py:179
msgid "Edit"
msgstr "Редактировать"

#. Translators: Label for the delete item in the context menu.
#: clip_manager/__init__.py:181
msgid "Delete"
msgstr "Удалить"

#: clip_manager/__init__.py:187
msgid "Unpin"
